<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.in.lab">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Lab"
        android:usesCleartextTraffic="true">
        <activity
            android:name=".activity.AuthenticationActivity"
            android:exported="true" />
        <activity
            android:name=".activity.TestRequestActivity"
            android:exported="true" />
        <activity
            android:name=".activity.MyReviewActivity"
            android:exported="true" />
        <activity
            android:name=".activity.MyWalletActivity"
            android:exported="true" />
        <activity
            android:name=".activity.AccountSettingActivity"
            android:exported="true" />
        <activity
            android:name=".activity.SplashActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".activity.SignUpActivity"
            android:exported="true" />
        <activity
            android:name=".activity.ActivityLogin"
            android:exported="true" />
        <activity
            android:name=".activity.ProfileSettingActivity"
            android:exported="true" />
        <activity
            android:name=".activity.MainActivity"
            android:exported="true" />
    </application>

</manifest>